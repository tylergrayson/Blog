<div class="form-group">
    <label for="firstName">First Name</label>
    <input required value="<%= member.firstName %>" type="text" name="firstName" id="firstName" class="form-control">
</div>

<div class="form-group">
    <label for="lastName">Last Name</label>
    <input required value="<%= member.lastName %>" type="text" name="lastName" id="lastName" class="form-control">
</div>

<div class="form-group">
    <label for="emailAddress">Email Address</label>
    <input required value="<%= member.emailAddress %>" type="text" name="emailAddress" id="emailAddress" class="form-control">
</div>

<div class="form-group">
    <label for="password">Password</label>
    <input required value="<%= member.password %>" type="text" name="password" id="password" class="form-control">
</div>

<a href="/" class="btn btn-secondary">Cancel</a>
<button type="submit" class="btn btn-primary">Register</button>